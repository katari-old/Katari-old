<!DOCTYPE HTML PUBLIC "-//W3C//DTD HTML 4.01//EN"
  "http://www.w3.org/TR/html4/strict.dtd">

<html>

  <head>
    <title>${title}</title>

    <meta http-equiv="Content-Type" content="text/html; charset=UTF-8" />

    <!-- caching pragmas, forces no caching in all known situations.
    Beware, not all proxies support http cache control, use http headers
    instead -->
    <meta http-equiv='pragma' content='no-cache'>
    <meta http-equiv='cache-control' content='no-cache'>
    <meta http-equiv='expires' content='0'>

    ${head}
    <link rel="stylesheet" type="text/css" media="screen"
        href="${baseweb}/module/decorator/css/katari.css">
    <link rel="icon" type="image/vnd.microsoft.icon"
        href="${baseweb}/module/decorator/image/katari.ico">
  </head>

  <body>
    <div id="header">
      <!-- Header starts here -->
      <#include "header.dec">
      <!-- Header ends here -->
    </div>
    <div id="frame">
      <ul id="mainMenu">
        <!-- Main menu starts here -->
        <!-- Main menu ends here -->
      </ul>
      <div id="section">
        <!-- Body begins here -->
        ${body}
        <!-- body ends here -->
      </div>
    </div>
    <div id="footer">
      <!-- Footer begins here -->
      <#include "footer.dec">
      <!-- Footer ends here -->
    </div>
  </body>

<#-- vim: set ts=2 et sw=2 filetype=html ai: -->
</html>

